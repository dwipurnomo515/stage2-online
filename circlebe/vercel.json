{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/index.js"
        }
    ],
    "env": {
        "NODE_VERSION": "20.x",
        "DATABASE_URL": "@DATABASE_URL",
        "DIRECT_URL": "@DIRECT_URL"
    }
}